{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "The AWS CloudFormation template for this Serverless application",
  "Resources": {
    "ServerlessDeploymentBucket": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketEncryption": {
          "ServerSideEncryptionConfiguration": [
            {
              "ServerSideEncryptionByDefault": {
                "SSEAlgorithm": "AES256"
              }
            }
          ]
        }
      }
    },
    "ServerlessDeploymentBucketPolicy": {
      "Type": "AWS::S3::BucketPolicy",
      "Properties": {
        "Bucket": {
          "Ref": "ServerlessDeploymentBucket"
        },
        "PolicyDocument": {
          "Statement": [
            {
              "Action": "s3:*",
              "Effect": "Deny",
              "Principal": "*",
              "Resource": [
                {
                  "Fn::Join": [
                    "",
                    [
                      "arn:",
                      {
                        "Ref": "AWS::Partition"
                      },
                      ":s3:::",
                      {
                        "Ref": "ServerlessDeploymentBucket"
                      },
                      "/*"
                    ]
                  ]
                },
                {
                  "Fn::Join": [
                    "",
                    [
                      "arn:",
                      {
                        "Ref": "AWS::Partition"
                      },
                      ":s3:::",
                      {
                        "Ref": "ServerlessDeploymentBucket"
                      }
                    ]
                  ]
                }
              ],
              "Condition": {
                "Bool": {
                  "aws:SecureTransport": false
                }
              }
            }
          ]
        }
      }
    },
    "DemoLogGroup": {
      "Type": "AWS::Logs::LogGroup",
      "Properties": {
        "LogGroupName": "/aws/lambda/docker-selenium-lambda-prod-demo"
      }
    },
    "DemoLambdaFunction": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "Code": {
          "ImageUri": "238589881750.dkr.ecr.us-east-1.amazonaws.com/serverless-docker-selenium-lambda-prod@sha256:93916c7adbd56b11cb01817c2c625217a3828d732c6359068d56d5f943af9be2"
        },
        "PackageType": "Image",
        "FunctionName": "docker-selenium-lambda-prod-demo",
        "MemorySize": 2048,
        "Timeout": 900,
        "Environment": {
          "Variables": {
            "DATA_BUCKET_NAME": "tabroom-summaries-data-bucket",
            "OPEN_AI_KEY_SECRET_NAME": "openai_auth_key",
            "SNS_TOPIC_ARN": "arn:aws:sns:us-east-1:238589881750:summary_generation_topic"
          }
        },
        "Role": "arn:aws:iam::238589881750:role/summary_lambda_role"
      },
      "DependsOn": [
        "DemoLogGroup"
      ]
    },
    "DemoLambdaVersion4ETrdD78XONeRiNwehfPO0cFpHgHBDP8K5ZHWATs": {
      "Type": "AWS::Lambda::Version",
      "DeletionPolicy": "Retain",
      "Properties": {
        "FunctionName": {
          "Ref": "DemoLambdaFunction"
        },
        "CodeSha256": "93916c7adbd56b11cb01817c2c625217a3828d732c6359068d56d5f943af9be2"
      }
    }
  },
  "Outputs": {
    "ServerlessDeploymentBucketName": {
      "Value": {
        "Ref": "ServerlessDeploymentBucket"
      },
      "Export": {
        "Name": "sls-docker-selenium-lambda-prod-ServerlessDeploymentBucketName"
      }
    },
    "DemoLambdaFunctionQualifiedArn": {
      "Description": "Current Lambda function version",
      "Value": {
        "Ref": "DemoLambdaVersion4ETrdD78XONeRiNwehfPO0cFpHgHBDP8K5ZHWATs"
      },
      "Export": {
        "Name": "sls-docker-selenium-lambda-prod-DemoLambdaFunctionQualifiedArn"
      }
    }
  }
}